{% extends 'InstacountInstacountBundle::layout.html.twig' %}
{% block body %}
<div data-role="page" data-add-back-btn="true">
        <header class="admin" data-position="fixed" data-role="header">
            <a data-transition="flip" data-ajax="false" href=" {{ path('InstacountInstacountBundle_homepage') }}" data-role="button" data-icon="arrow-l" class="ui-btn-left" data-iconpos="notext"></a>
            
           
        </header>
        <div class="content" data-theme="b" data-role="content">   
         
<form action="{{ path('login_check') }}" method="post">
    <label for="username">Användarnamn</label>
    <input type="text" id="username" name="_username" value="{{ last_username }}" />

    <label for="password">Lösenord:</label>
    <input type="password" id="password" name="_password" />

    {#
        If you want to control the URL the user
        is redirected to on success (more details below)
        <input type="hidden" name="_target_path" value="/account" />
    #}

    <button type="submit">login</button>
</form>
{% endblock %}